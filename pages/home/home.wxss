/* pages/home/home.wxss */
.pageWrap{
  background-color:#f2f3f9;
}
.banner{
  width:auto;
  height:300rpx;
}
.banner image{
  width:100%;
  height:300rpx;
}
.tagboxWrap{
  height:380rpx;
  width:auto;
  display: flex;
  /* justify-content: space-around; */
  justify-content: center;
  margin:20rpx 0;
  /* padding:0 10rpx; */
}
.tagbox{
  height:380rpx;
  width:330rpx;
  background-color: #ffffff;
	border-radius: 5rpx;
	border: solid 1rpx #dde4fa;
}
.tagbox .title1{
  color:#666;
  font-size:30rpx;
  font-weight:700;
  text-align: center;
  line-height:80rpx;
}
.tagbox .title1 text{
  color:#dde4fa;
  font-weight:700;
}
.tagWrap{
  height:300rpx;
  width:auto;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  align-content: space-around;
}
.tag{
  width:50%;
  height:50%;
  text-align: center;
}
/* .tag image{
  width:100rpx;
  height:100rpx;
  display: block;
  margin:0 auto;
} */
.tag text{
  display: block;
  color:#333;
  font-size:28rpx;
}
.activity{
  width:auto;
  height:100rpx;
  display: flex;
  align-items: center;
  background-color:#fff;
}
.activity image.title{
  width:170rpx;
  height:100rpx;
  flex:1 1 none;
}
.activity image.next{
  width:80rpx;
  height:80rpx;
  flex:1 1 none;
  margin-left:-30rpx;
  margin-right:10rpx;
}
.activity text{
  padding-left:25rpx;
  font-size:24rpx;
  color: #6d6f78;
  flex:1 1 auto;
}
.zhxy{
  height:145rpx;
  width:auto;
  margin:20rpx 0;
  background-color:#fff;
  background-repeat: no-repeat;
  background-size:auto 100%;
  background-position:right top;
  background-image:url('data:image/png;base64,');
  display: flex;
  align-content:center;
  flex-wrap: wrap;
  padding-left:30rpx;
}
.zhxy .name{
  font-size:36rpx;
  color:#000;
  width:100%;
}
.zhxy .name text{
  color: #2f9bfe;
}
.zhxy .intro{
  font-size:24rpx;
  width:100%;
  color: #a6a6a6;
}
.xyhdWrap{
  background-color:#fff;
  padding:20rpx 20rpx 0;
}
.xyhdWrap .title{
  font-size:30rpx;
  height:35rpx;
  width:100%;
  border-left:12rpx solid #2f9bfe;
  color: #666666;
  line-height:35rpx;
  padding-left:10rpx;
  margin-bottom:20rpx;
}
.xyhdList{
  height:90rpx;
  border-top:1px solid #dce6ec;
  padding:20rpx 10rpx;
  display: flex;
}
.xyhdList image{
  height:90rpx;
  width:140rpx;
  flex:0 0 auto;
}
.xyhdList .content{
  height:90rpx;
  width:auto;
  flex:1 1 auto;
  line-height:45rpx;
  position:relative;
  color: #333333;
  font-size:30rpx;
  padding-left:30rpx;
}
.xyhdList .content .date{
  position:absolute;
  right:0;
  bottom:0;
  line-height:45rpx;
  color: #666666;
  font-size:18rpx;
}
.end{
  text-align: center;
  color: #333333;
  font-size:30rpx;
  background-color:#f2f3f9;
  margin:0 -20rpx;
}
.weui-loadmore{
  margin:5rpx auto;
  width:100%;
  border-top:1px solid #dce6ec;
}
.empty{
  font-size:24rpx;
  width:100%;
  text-align: center;
  line-height:200rpx;
  color: #a6a6a6;
}