/* pages/login/identity/identity.wxss */
page{
  width:100%;
  height:100%;
}
.pageWrap{
  width:100%;
  height:100%;
  background-repeat: no-repeat;
  background-size:cover;
  background-position: center bottom;
  background-image:url("data:image/png;base64,");
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}
.pageWrap .title{
  font-size:46rpx;
  color: #333333;
  line-height:150rpx;
  margin-bottom:34rpx;
}
.pageWrap .idbox{
  width:220rpx;
  height:300rpx;
  position:relative;
  text-align: center;
  margin-bottom:20rpx;
}
.pageWrap .idbox .photo{
  width:220rpx;
  height:220rpx;
  border-radius: 50%;
  overflow: hidden;
  display: block;
  box-sizing: border-box;
  border:1px solid #fff;
}
.pageWrap .idbox .photo.active{
  border-color:#f5664a;
}
.pageWrap .idbox .current{
  width:120rpx;
  height:120rpx;
  position:absolute;
  top:220rpx;
  left:50%;
  margin-left:-60rpx;
  margin-top:-60rpx;
}
.pageWrap .idbox text{
  font-size:28rpx;
  line-height: 80rpx;
  color: #7580ff;
}
page .pageWrap .btnl{
  width: 300rpx;
	height: 72rpx;
  line-height:72rpx;
	background-color: #ffffff;
	border-radius: 36rpx;
  border: solid 1px #1082ff;
  color: #1082ff;
  font-size:32rpx;
  margin-top:20rpx;
}
/* 有关弹窗的样式 */
.mask{
  position:fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  z-index: 6;
  background-color:rgba(0,0,0,0.7);
}
.modal{
  position:fixed;
  width:540rpx;
  height:610rpx;
  left:50%;
  top:50%;
  margin-top:-305rpx;
  margin-left:-270rpx;
  z-index: 66;
  background-repeat: no-repeat;
  background-size:cover;
  background-position: center;
  background-image: url("data:image/png;base64,");
}
.modal .close{
  width:62rpx;
  height:62rpx;
  position: absolute;
  right:-31rpx;
  top:-31rpx;
}
.modal .modalTitle{
  color:#000;
  font-size:32rpx;
  line-height:86rpx;
  font-weight: 700;
  text-align: center;
}
.modal .photoBoxWrap{
  height:380rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding:0 20rpx;
}
.modal .idbox{
  width:180rpx;
  height:260rpx;
  position:relative;
  text-align: center;
  /* margin-bottom:20rpx; */
}
.modal .idbox .photo{
  width:180rpx;
  height:180rpx;
  border-radius: 50%;
  overflow: hidden;
  display: block;
  box-sizing: border-box;
  border:1px solid #fff;
}
.modal .idbox .photo.active{
  border-color:#f5664a;
}
.modal .idbox .current{
  width:120rpx;
  height:120rpx;
  position:absolute;
  top:180rpx;
  left:50%;
  margin-left:-60rpx;
  margin-top:-60rpx;
}
.modal .idbox text{
  font-size:26rpx;
  line-height: 80rpx;
  color: #8f8f8f;
}
page .modal .btn{
  width: 300rpx;
	height: 72rpx;
  line-height:72rpx;
	background-color: #4a8df5;
  border-radius: 36rpx;
  border: none;
  color: #fff;
  font-size:32rpx;
  /* margin-top:20rpx; */
}